{
    "name": "jedo/contao-guestbook-bundle",
    "keywords": ["contao","guestbook"],
    "type": "contao-bundle",
    "description": "Contao 4 Simple Guestbook Extension",
    "license": "LGPL-3.0+",
    "authors": [
        {
          "name": "Jens Doberenz"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "contao/core-bundle": "^4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
          "Jedo\\GuestBookBundle\\": "src/"
        },
        "classmap": [
          "src/Resources/contao"
        ],
        "exclude-from-classmap": [
          "src/Resources/contao/config",
          "src/Resources/contao/dca",
          "src/Resources/contao/languages",
          "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Jedo\\GuestBookBundle\\ContaoManager\\Plugin"
    } 
}