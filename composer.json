{
    "name": "seefahrer/contao-guestbook-bundle",
    "keywords": ["contao4","guestbook","bundle"],
    "type": "contao-bundle",
    "description": "Contao 4 Simple Guestbook Extension",
    "license": "LGPL-3.0+",
    "authors": [
        {
          "name": "Georg Scherg"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "contao/core-bundle":"^4.13 || ^5.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
          "Seefahrer\\GuestBookBundle\\": "src/"
        },
        "classmap": [
          "src/Resources/contao"
        ],
        "exclude-from-classmap": [
          "src/Resources/contao/config",
          "src/Resources/contao/dca",
          "src/Resources/contao/languages",
          "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Seefahrer\\GuestBookBundle\\ContaoManager\\Plugin"
    }
}
